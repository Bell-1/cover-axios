{
    "name": "cover-axios",
    "version": "0.3.1",
    "description": "基于axios的二次封装, 支持TS",
    "main": "./lib/cover-axios.js",
    "exports": {
        ".": {
            "import": "./lib/cover-axios.es.js",
            "require": "./lib/cover-axios.umd.js"
        },
        "./style": "./lib/style.css",
        "./*": "./*"
    },
    "scripts": {
		"preinstall": "npx only-allow pnpm",
        "prepublish": "pnpm run build",
        "genVersion": "node ./build/version.js",
        "build": "pnpm run genVersion && webpack --config ./webpack.config.js"
    },
    "typings": "types/index.d.ts",
    "author": "Bell",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Bell-1/cover-axios/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Bell-1/cover-axios"
    },
    "keywords": [
        "xhr",
        "http",
        "ajax",
        "promise",
        "node"
    ],
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "axios": "^0.26.0",
        "babel-loader": "^8.2.3",
        "progress-bar-webpack-plugin": "^2.1.0",
        "webpack": "^5.69.1",
        "webpack-cli": "^4.9.2"
    },
    "jsdelivr": "dist/axios.min.js",
    "unpkg": "dist/axios.min.js",
    "peerDependencies": {
        "axios": "^0.26.0"
    }
}